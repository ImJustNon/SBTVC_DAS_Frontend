<body>
    <!--Main Navigation-->
    <header>
      <!-- Navbar -->
      <%- include("./partials/navbar.ejs") %>
      <!-- Navbar -->
    </header>
    <!--Main Navigation-->

    <!-- script -->
    <%- include("./partials/script.ejs") %>
    <!-- script -->


    <!-- Main page -->
    <% if(PAGE === "HOME"){ %>
      <%- include("./pages/home.ejs") %>
    <% } else if(PAGE === "FORM"){ %>
